<?php

namespace Dahab\AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity(repositoryClass="Dahab\AppBundle\Entity\PasswordRepository")
 * @ORM\Table(name="passwords")
 */
class Password
{
    /**
     * @var string
     */
    private $password;

    /**
     * @var boolean
     */
    private $valid;

    /**
     * @var integer
     */
    private $id;


    /**
     * Set password
     *
     * @param string $password
     * @return Passwords
     */
    public function setPassword($password)
    {
        $this->password = $password;

        return $this;
    }

    /**
     * Get password
     *
     * @return string 
     */
    public function getPassword()
    {
        return $this->password;
    }

    /**
     * Set valid
     *
     * @param boolean $valid
     * @return Passwords
     */
    public function setValid($valid)
    {
        $this->valid = $valid;

        return $this;
    }

    /**
     * Get valid
     *
     * @return boolean 
     */
    public function getValid()
    {
        return $this->valid;
    }

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }
}
